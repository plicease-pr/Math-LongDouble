/*
Check whether erfl and isnanl can be linked.
*/

#include <stdio.h>
#include <math.h>

int main(void) {
 long double rop, op = 0.6L;
 int ret;

 rop = erfl(op);
 ret = isnanl(op);

 if(rop > 0.6038L && rop < 0.6039L) printf("rop is set to a sane value\n");
 if(!ret) printf("op is set to the correct value\n");

 return 0;
}

